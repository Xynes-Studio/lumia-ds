{
    "name": "@lumia/icons",
    "version": "0.0.0",
    "description": "Icon registry utilities for Lumia DS",
    "sideEffects": false,
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./generated/*": {
            "types": "./generated/*.d.ts",
            "import": "./generated/*.tsx"
        }
    },
    "scripts": {
        "build": "pnpm build:icons && tsup",
        "build:icons": "svgr --config-file svgr.config.js svg && node scripts/generate-index.js",
        "dev": "tsup --watch",
        "test": "vitest run --coverage"
    },
    "dependencies": {
        "lucide-react": "^0.474.0"
    },
    "peerDependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@svgr/cli": "^8.1.0",
        "@svgr/plugin-jsx": "^8.1.0",
        "@svgr/plugin-prettier": "^8.1.0",
        "@svgr/plugin-svgo": "^8.1.0",
        "@testing-library/react": "^16.3.0",
        "@types/node": "^22.19.2",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitest/coverage-v8": "^2.1.9",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "tsup": "^8.0.0",
        "vite": "^7.2.4",
        "vitest": "^2.1.9"
    }
}