{
  "name": "@lumia/cli",
  "version": "0.0.0",
  "private": true,
  "description": "CLI utilities for Lumia DS",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "bin": {
    "lumia": "./bin/lumia.js",
    "lumia-resource": "./bin/lumia-resource.js",
    "lumia-icon-import": "./bin/lumia-icon-import.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run"
  },
  "dependencies": {
    "commander": "^13.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.9",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  },
  "license": "ISC"
}