                "version": 1,
                "children": [
                    {
                        "type": "text",
                        "text": "One of the simplest yet most useful hooks is `useToggle`. Here is how you can implement it:",
                        "format": 0,
                        "mode": "normal",
                        "style": "",
                        "detail": 0,
                        "version": 1
                    }
                ]
            },
            {
                "type": "code",
                "language": "typescript",
                "version": 1,
                "children": [
                    {
                        "type": "text",
                        "text": "import { useState, useCallback } from 'react';\n\nexport function useToggle(initialValue: boolean = false) {\n  const [value, setValue] = useState(initialValue);\n  const toggle = useCallback(() => setValue(v => !v), []);\n  return [value, toggle] as const;\n}",
                        "format": 0,
                        "mode": "normal",
                        "style": "",
                        "detail": 0,
                        "version": 1
                    }
